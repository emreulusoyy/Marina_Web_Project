@using EntityLayer.Concrete
@model List<Hakkında>

<div class="p-5 mb-4 bg-light rounded-3">
    @foreach(var x in Model)
    {
        

    <div class="container-fluid py-5">
                            <h3 key="1">@x.HakkındaBaslik</h3>
                            <p key="9" class="col-md-8">
                               @x.HakkındaAcıklama
                            </p>
                            <button key="13" class="btn btn-primary btn-lg" type="button">Görüntüle</button>
                        </div>
    }
                    </div>
