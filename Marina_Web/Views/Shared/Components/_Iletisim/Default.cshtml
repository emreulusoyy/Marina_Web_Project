        <a href="" class="list-group-item list-group-item-action d-flex gap-3 py-3"
           aria-current="true">
            <i class="bi bi-telephone-outbound fa-2x rounded-circle flex-shrink-0" width="32"
               height="32"></i>
            <div class="d-flex gap-2 w-100 justify-content-between">
                <div>
                    <h6 class="mb-0">@x.GsmNo</h6>
                    <p class="mb-0 opacity-75">@x.OfisNo</p>
                </div>
            </div>
